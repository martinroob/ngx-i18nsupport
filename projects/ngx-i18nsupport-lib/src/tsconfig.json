{
  "extends": "../tsconfig.lib.json",
  "compilerOptions": {
    "baseUrl": "tsconfig",
    "outDir": "../../../dist/ngx-i18nsupport-lib/src",
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "target": "es6",
    "types": [
      "jasmine",
      "node"
    ]
  },
  "include": [
    "api/**/*",
    "impl/**/*"
  ],
  "exclude": [
  ]
}
